{
  "name": "find-installed-packages",
  "version": "0.0.0-development",
  "description": "Find installed packages",
  "homepage": "https://github.com/unional/find-installed-packages",
  "bugs": {
    "url": "https://github.com/unional/find-installed-packages/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/unional/find-installed-packages.git"
  },
  "license": "MIT",
  "author": {
    "name": "Homa Wong (unional)",
    "email": "homawong@gmail.com"
  },
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "clean": "rimraf lib",
    "dc": "npm run dependency-check",
    "dependency-check": "dependency-check . --unused --no-dev && dependency-check . --missing --no-dev",
    "lint": "eslint --ext=js,ts .",
    "test": "jest",
    "verify": "npm run lint && npm run build && npm run dependency-check && jest --coverage",
    "watch": "jest --watch"
  },
  "dependencies": {
    "bluebird": "^3.5.5",
    "find": "^0.3.0",
    "temp": "^0.9.1",
    "unpartial": "^0.6.3",
    "walkdir": "^0.4.1"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.27",
    "@types/find": "^0.2.1",
    "@types/temp": "^0.8.34",
    "@unional/devpkg-node": "^1.4.2",
    "assertron": "^7.1.2"
  }
}
